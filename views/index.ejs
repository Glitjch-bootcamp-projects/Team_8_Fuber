<%- include('./headers_main.ejs') %>
<body>
  <main>

    <div> 
      <h1 class="frontPageTitle">You Order, We Deliver</h1>
    </div>

      <div class="addressSumbit">

        <div class="addressBarTime">
          <input class='addressBar' type="address" name="address" placeholder="Enter Your Address">
            <button class="Schedule"><i class="fa-solid fa-calendar"></i></button> 
        </div>

      <div class="Time">
        <input type="date" id="date" name="scheduleDate" value="Now" min="2021" max="2022"> 
        <input type="time" id="time" name="scheduleTime" value="Now" min="00:00" max="23:59"> 
      </div>

      <form action = '/address' method="GET"></form>
        <button class='addressBtn' type="submit">Find Restaurants In Your Area</button>
      </form>

      </div>  

  </main>
</body>

</html>