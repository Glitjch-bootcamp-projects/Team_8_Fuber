<!DOCTYPE html>
<html lang="en">

<head>
  <title>Füber Feasts</title>
  <meta charset="UTF-8" />
  <link rel="icon" href="https://github.com/glitjch/Team_8_Fuber/blob/html/docs/F%C3%BCberFeastsLogo.png?raw=true">
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
  <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
  <link rel="stylesheet" href="/styles/restaurants.css" type="text/css" />
  <link rel="stylesheet" href="/styles/_header.css" type="text/css" />
  <link rel="stylesheet" href="/styles/_popupA.css" type="text/css" />
  <link rel="stylesheet" href="/styles/_cart.css" type="text/css"/>

  <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
  <script type="text/javascript" src="/scripts/app.js"></script>
  <script type="text/javascript" src="/scripts/add-to-cart.js"></script>
  <script type="text/javascript" src="/scripts/change-address.js"></script>
</head>

<body id="body-tj">
  <!-- Side Bar -->
  <div class="sidebar">
    <div class="sidebar-header">
      All Stores
    </div>
    <div class="sidebar-subheader">
      Sort
    </div>
    <div class="sidebar-text">
      <span><i class="icon" data-feather="thumbs-up"></i>Picked for you</span>
      <span><i class="icon" data-feather="heart"></i>Most popular</span>
      <span><i class="icon" data-feather="star"></i>Rating</span>
      <span><i class="icon" data-feather="clock"></i></img>Delivery time</span>
    </div>
    <div class="sidebar-subheader">
      Price range
    </div>
    <div class="sidebar-price">
      <button id="p1" type="button">$</button>
      <button id="p2" type="button">$$</button>
      <button id="p3" type="button">$$$</button>
      <button id="p4" type="button">$$$$</button>
    </div>
    <div class="sidebar-price-range">
      Max. Delivery Fee
    </div>
    <div class="sidebar-slide-container">
      <input type="range" min="1" max="100" value="50" class="slider" id="myRange">
    </div>
    <div class="sidebar-price-range-points">
      <span>$1</span>
      <span>$3</span>
      <span>$5+</span>
    </div>
    <div class="sidebar-subheader">
      Dietary
    </div>
    <div class="sidebar-dietary">
      <span><i class="icon" data-feather="feather"></i>Vegetarian</span>
      <span><i class="icon" data-feather="heart"></i>Vegan</span>
      <span><img class="icon" src="https://cdn-icons-png.flaticon.com/128/1167/1167712.png"></img>Allergy
        friendly</span>
      <span><img class="icon"
          src="https://cdn-icons.flaticon.com/png/128/2276/premium/2276190.png?token=exp=1644666005~hmac=2ef90f4c7b0b4a23d544a14afe0417d1"></img>Gluten-free</span>
      <span><img class="icon"
          src="https://cdn-icons.flaticon.com/png/128/5862/premium/5862723.png?token=exp=1644665912~hmac=c80e236303102da9c4c38568a4927f6a"></img>Keto</span>
    </div>
  </div>
  </div>

  <div id="header-tj">
    <%-include('partials/_header')%>
  </div>
  <!-- Options -->


  <!-- Orders Near You -->
  <div class="listings">
    <div class="container">
      <div class="header">
        <div class="header-title">
          <h2>Orders near you</h2>
          <span>Your neighborhood's latest orders</span>
        </div>
        <div class="header-view-options">
          <div class="view-more">
            <span class="arrow left">
              <i data-feather="arrow-left"></i>
            </span>
            <span class="arrow right darker">
              <i data-feather="arrow-right"></i>
            </span>
          </div>
        </div>
      </div>
      <div class="listings-grid">
        <div class="listings-grid-element">
          <div class="image">
            <img
              src="https://images.unsplash.com/photo-1598866594230-a7c12756260f?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1608&q=80"
              alt="Listing pic">
          </div>
          <div class="listings-text">
            <div class="listings-text-title">
              <h3>Pasta Palace</h3>
              <div class="listings-info">
                <span>2 items • $19.03 • 3 mins ago</span>
              </div>
            </div>
            <div class="listings-rating">
              <span>4.8</span>
            </div>
          </div>
        </div>
        <div class="listings-grid-element">
          <div class="image">
            <img
              src="https://images.unsplash.com/photo-1596662951482-0c4ba74a6df6?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=774&q=80"
              alt="Listing pic">
          </div>
          <div class="listings-text">
            <div class="listings-text-title">
              <h3>Hamburger Hashery </h3>
              <div class="listings-info">
                <span>4 items • $43.28 • 19 mins ago</span>
              </div>
            </div>
            <div class="listings-rating">
              <span>4.2</span>
            </div>
          </div>
        </div>
        <div class="listings-grid-element">
          <div class="image">
            <img
              src="https://images.unsplash.com/photo-1611143669185-af224c5e3252?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=2832&q=80"
              alt="Listing pic">
          </div>
          <div class="listings-text">
            <div class="listings-text-title">
              <h3>Super Sushi</h3>
              <div class="listings-info">
                <span>1 item • $5.83 • 23 mins ago</span>
              </div>
            </div>
            <div class="listings-rating">
              <span>4.0</span>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>


  <!-- Popular Near You -->
  <div class="listings">
    <div class="container">
      <div class="header">
        <div class="header-title">
          <h2>Popular near you</h2>
        </div>
        <div class="header-view-options">
          <div class="view-all">
            <span>See all</span>
          </div>
          <div class="view-more">
            <span class="arrow left">
              <i data-feather="arrow-left"></i>
            </span>
            <span class="arrow right darker">
              <i data-feather="arrow-right"></i>
            </span>
          </div>
        </div>
      </div>
      <div class="listings-grid rest-container" id="kebab-kingdom">
        <% for (let restaurant of restaurants) { %>
          <div class="listings-grid-element <%= restaurant.price_range %>">
            <div class="image">
              <img src=<%=restaurant.image %> alt="Listing pic">
            </div>
            <div class="listings-text">
              <div class="listings-text-title">
                <h3>
                  <%= restaurant.name %>
                </h3>
                <div class="listings-info">
                  <span> $<%= restaurant.delivery_fee / 100 %> delivery fee • <%= restaurant.delivery_time - 15 %>-<%=
                          restaurant.delivery_time %> min</span>
                </div>
              </div>
              <div class="listings-rating">
                <span>
                  <%= restaurant.rating %>
                </span>
              </div>
            </div>
          </div>
          <% } %>
      </div>
    </div>
  </div>
  <script src="https://unpkg.com/feather-icons"></script>
  <script>
    feather.replace()
  </script>
</body>

</html>
